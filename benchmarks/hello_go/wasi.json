{
	"llvm-target":   "wasm32-unknown-wasi",
	"cpu":           "generic",
	"features":      "+bulk-memory,+nontrapping-fptoint,+sign-ext",
	"build-tags":    ["tinygo.wasm", "wasi", "runtime_memhash_leveldb"],
	"goos":          "linux",
	"goarch":        "arm",
	"linker":        "wasm-ld",
	"libc":          "wasi-libc",
	"scheduler":     "none",
	"default-stack-size": 1048576,
	"cflags": [
		"-mbulk-memory",
		"-mnontrapping-fptoint",
		"-msign-ext"
	],
	"ldflags": [
		"--stack-first",
		"--no-demangle",
        "--gc-sections",
        "--print-gc-sections",
        "--initial-memory=4194304",
        "--max-memory=4194304",
        "--lto-O3"
	],
	"emulator":      "wasmtime {}",
	"wasm-abi":      "generic"
}
